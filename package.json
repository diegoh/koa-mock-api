{
  "name": "@diegoh/koa-mock-api",
  "version": "1.0.0",
  "description": "A koa app that returns example requests to test healthchecks, timeouts and error responses",
  "main": "dist/index.js",
  "prettier": "@diegoh/eslint-config-prettier-typescript-node/src/prettier.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev src/index.ts",
    "lint": "eslint **/*.ts",
    "lint:fix": "eslint **/*.ts --fix",
    "test:unit": "NODE_ENV=test jest --testPathPattern src",
    "test:coverage": "npm run test:unit -- --coverage",
    "test:integration": "NODE_ENV=test jest --testPathPattern 'test'",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diegoh/koa-mock-api.git"
  },
  "author": "Diego Herrera",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/diegoh/koa-mock-api/issues"
  },
  "homepage": "https://github.com/diegoh/koa-mock-api#readme",
  "dependencies": {
    "http-status-codes": "^1.4.0",
    "koa": "^2.11.0",
    "koa-logger": "^3.2.1"
  },
  "devDependencies": {
    "@diegoh/eslint-config-prettier-typescript-node": "^1.1.1",
    "@diegoh/tsconfig": "^1.0.5",
    "@types/koa-logger": "^3.1.1",
    "@types/node": "^13.13.5",
    "@shopify/jest-koa-mocks": "^2.2.2",
    "@types/jest": "^25.2.1",
    "@types/supertest": "^2.0.9",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "ts-jest": "^25.5.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3",
    "eslint": "^6.8.0",
    "prettier": "^2.0.5"
  },
  "keywords": [
    "node",
    "koa",
    "docker",
    "mock-api"
  ]
}
